do shell script "open x-apple.systempreferences:com.apple.preferences.AppleIDPrefPane?iCloud"delay 2tell application "System Events"	tell window "iCloud" of process "System Settings"		delay 1		click button 1 of group 3 of scroll area 1 of group 1 of group 2 of splitter group 1 of group 1		delay 1		set theCheckbox to checkbox 1 of group 1 of scroll area 1 of group 1 of sheet 1		if theCheckbox's value as boolean then			click checkbox 1 of group 1 of scroll area 1 of group 1 of sheet 1			delay 1			click button 2 of sheet 1 of sheet 1		end if		delay 1		click button 1 of group 1 of sheet 1		delay 1	end tellend telltell application "System Settings" to quit